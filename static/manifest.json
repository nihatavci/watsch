{
  "name": "Watsch App",
  "short_name": "Watsch",
  "description": "Monitor and manage your tasks efficiently with Watsch App, designed to streamline your productivity.",
  "id": "/",
  "start_url": "/?utm_medium=PWA&utm_source=launcher",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    { "src": "/icon1.png", "sizes": "192x192", "type": "image/png" },
    { "src": "/icon2.png", "sizes": "512x512", "type": "image/png" },
    { "src": "/icon-maskable.png", "sizes": "512x512", "type": "image/png", "purpose": "maskable" }
  ],
  "screenshots": [
    { "src": "/screenshot1.png", "sizes": "640x480", "type": "image/png" },
    { "src": "/screenshot2.png", "sizes": "800x600", "type": "image/png" }
  ],
  "launch_handler": {
    "dynamic_url": "index.html?action=launch"
  },
  "lang": "en",
  "categories": ["productivity", "utilities"],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [{
        "name": "file",
        "accept": ["image/*"]
      }]
    }
  }
}
